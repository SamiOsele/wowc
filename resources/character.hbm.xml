<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"https://hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.mycompany.editor">

	<class name="Character" table="CHARACTER">
		<id name="name" type="string">
			<column name="name" />
		</id>
		<property name="gender" type="string">
			<column name="gender"></column>
		</property>
		<property name="race" type="string">
			<column name="race"></column>
		</property>
		<property name="clazz" type="string">
			<column name="class"></column>
		</property>
		<many-to-one name="user" class="User">
			<column name="username" not-null="true"></column>
		</many-to-one>
	</class>

</hibernate-mapping>